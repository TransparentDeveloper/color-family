{
  "name": "color-family",
  "version": "0.1.2",
  "description": "Provides a range of color families for application design.",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    }
  },
  "scripts": {
    "build": "rm -fr dist/* && tsc -p tsconfig-esm.json && tsc -p tsconfig-cjs.json && bash ./fixup.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TransparentDeveloper/color-family.git"
  },
  "keywords": [
    "react",
    "vue",
    "color",
    "random",
    "css",
    "color family"
  ],
  "author": "yunshin <2younsin@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TransparentDeveloper/color-family/issues"
  },
  "homepage": "https://github.com/TransparentDeveloper/color-family#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  }
}
